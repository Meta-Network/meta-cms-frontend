{
  "name": "meta-space-console",
  "version": "1.0.0",
  "private": true,
  "description": "Platform to generates and maintains Meta Space written in Ant Design Pro",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --custom-syntax postcss-less",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env UMI_ENV=dev umi dev",
    "start:test": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "serve": "umi-serve",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix \"src/**/*.less\" --custom-syntax postcss-less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-card": "^1.14.12",
    "@ant-design/pro-descriptions": "^1.6.8",
    "@ant-design/pro-form": "^1.18.3",
    "@ant-design/pro-layout": "^6.15.3",
    "@ant-design/pro-table": "^2.30.8",
    "@metaio/meta-signature-util-v2": "^2.3.0",
    "@simplewebauthn/browser": "^4.1.0",
    "@umijs/route-utils": "^2.0.4",
    "ahooks": "^2.10.11",
    "antd": "^4.14.0",
    "antd-img-crop": "^4.0.2",
    "base64-url": "^2.3.3",
    "classnames": "^2.2.6",
    "dexie": "^3.0.3",
    "dexie-react-hooks": "^1.0.7",
    "gun": "^0.2020.1235",
    "is-mobile": "^3.0.0",
    "isomorphic-form-data": "^2.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.33",
    "omit.js": "^2.0.2",
    "react": "^17.0.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.0.4",
    "react-spring": "^9.4.1",
    "socket.io-client": "^4.4.1",
    "store": "^2.0.12",
    "swr": "^1.2.1",
    "umi": "^3.5.20",
    "umi-serve": "^1.9.10",
    "use-wallet": "^0.13.6",
    "vditor": "3.8.11",
    "xss": "^1.0.11"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.144",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/store": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.9.0",
    "@umijs/fabric": "latest",
    "@umijs/openapi": "^1.1.14",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^2.1.0",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.1.5",
    "mockjs": "^1.0.1-beta3",
    "postcss": "8.4.4",
    "prettier": "^2.3.2",
    "puppeteer-core": "^13.0.1",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
